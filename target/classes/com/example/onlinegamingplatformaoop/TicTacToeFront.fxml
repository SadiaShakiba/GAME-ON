<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" styleClass="TTT" stylesheets="@CSS_File/Style.css" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.onlinegamingplatformaoop.HelloController">
   <children>
      <Text fill="WHITE" layoutX="215.0" layoutY="90.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Tic-Tac-Toe">
         <font>
            <Font name="Berlin Sans FB" size="34.0" />
         </font>
      </Text>
      <Button fx:id="singlePlayerBtn" layoutX="222.0" layoutY="138.0" mnemonicParsing="false" onAction="#singlePlayerMode" prefHeight="30.0" prefWidth="153.0" style="-fx-border-color: #ffffff; -fx-border-width: 2px 2px 2px 2px;" styleClass="navBtn" stylesheets="@CSS_File/Style.css" text="Play" textFill="WHITE">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
         <font>
            <Font name="Berlin Sans FB" size="14.0" />
         </font></Button>
      <Button fx:id="exitGame" layoutX="221.0" layoutY="229.0" mnemonicParsing="false" onAction="#exit" prefHeight="30.0" prefWidth="153.0" style="-fx-border-color: #ffffff; -fx-border-width: 2px 2px 2px 2px;" styleClass="navBtn" text="Exit" textFill="WHITE">
         <font>
            <Font name="Berlin Sans FB" size="14.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></Button>
      <Button fx:id="singlePlayerBtn1" layoutX="221.0" layoutY="185.0" mnemonicParsing="false" onAction="#singlePlayerMode" prefHeight="30.0" prefWidth="153.0" style="-fx-border-color: #ffffff; -fx-border-width: 2px 2px 2px 2px;" styleClass="navBtn" stylesheets="@CSS_File/Style.css" text="Game history" textFill="WHITE">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
         <font>
            <Font name="Berlin Sans FB" size="14.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
